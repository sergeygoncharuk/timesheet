# .env.example - Copy to .env and fill in your values
# These keys are required for Airtable integration

VITE_AIRTABLE_API_KEY=your_airtable_personal_access_token
VITE_AIRTABLE_BASE_ID=your_airtable_base_id
VITE_AIRTABLE_TABLE_ID=your_airtable_table_id

# Screenshot Machine API key for weather forecast screenshots
VITE_SCREENSHOTMACHINE_API_KEY=your_screenshotmachine_api_key

# n8n webhook URLs for tide data
VITE_N8N_TIDE_CONAKRY_URL=your_n8n_conakry_webhook_url
VITE_N8N_TIDE_KAMSAR_URL=your_n8n_kamsar_webhook_url
